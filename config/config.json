{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "wp-bienvenido-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpBienvenido/WpBienvenidoWebPart.js",
          "manifest": "./src/webparts/wpBienvenido/WpBienvenidoWebPart.manifest.json"
        }
      ]
    },
    "wp-carousel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpCarousel/WpCarouselWebPart.js",
          "manifest": "./src/webparts/wpCarousel/WpCarouselWebPart.manifest.json"
        }
      ]
    },
    "wp-noticias-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpNoticias/WpNoticiasWebPart.js",
          "manifest": "./src/webparts/wpNoticias/WpNoticiasWebPart.manifest.json"
        }
      ]
    },
    "wp-articulos-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpArticulos/WpArticulosWebPart.js",
          "manifest": "./src/webparts/wpArticulos/WpArticulosWebPart.manifest.json"
        }
      ]
    },
    "wp-galeria-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpGaleria/WpGaleriaWebPart.js",
          "manifest": "./src/webparts/wpGaleria/WpGaleriaWebPart.manifest.json"
        }
      ]
    },
    "wp-nav-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpNav/WpNavWebPart.js",
          "manifest": "./src/webparts/wpNav/WpNavWebPart.manifest.json"
        }
      ]
    },
    "wp-avisos-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wpAvisos/WpAvisosWebPart.js",
          "manifest": "./src/webparts/wpAvisos/WpAvisosWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WpBienvenidoWebPartStrings": "lib/webparts/wpBienvenido/loc/{locale}.js",
    "WpCarouselWebPartStrings": "lib/webparts/wpCarousel/loc/{locale}.js",
    "WpNoticiasWebPartStrings": "lib/webparts/wpNoticias/loc/{locale}.js",
    "WpArticulosWebPartStrings": "lib/webparts/wpArticulos/loc/{locale}.js",
    "WpGaleriaWebPartStrings": "lib/webparts/wpGaleria/loc/{locale}.js",
    "WpNavWebPartStrings": "lib/webparts/wpNav/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}